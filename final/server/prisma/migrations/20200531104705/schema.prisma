generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url = "***"
}

model User {
  id           Int     @default(autoincrement()) @id
  email        String?
  profileImage String?
  token        String?
  Trip         Trip[]
}

model Trip {
  id       Int  @default(autoincrement()) @id
  launchId String?
  User     User @relation(fields: [userId], references: [id])
  userId Int
}
