generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../store.sqlite"
}

model Users {
  id           Int       @default(autoincrement()) @id
  email        String?
  profileImage String?
  token        String?
  createdAt    DateTime?
  updatedAt    DateTime?
}

model Trips {
  id        Int       @default(autoincrement()) @id
  launchId  Int?
  userId    Int?
  updatedAt DateTime?
  createdAt DateTime?
}
